package prueba;

public class Main {

    public static void main(String[] args) {
        // write your code here
        int resultado = suma3num(2, 3, 4);
        System.out.println("El resultado de la suma es: " + resultado);

        Coche miCoche = new Coche();
        miCoche.incpuertas();
        miCoche.incpuertas();
        miCoche.incpuertas();
        miCoche.incpuertas();
    }

    public static int suma3num(int a, int b, int c) {
        return a + b + c;
    }
}

class Coche {

    public int puertas = 0;

    public void incpuertas(){
        this.puertas++;
        System.out.println("El numero de puertas es: " + puertas);
    }
